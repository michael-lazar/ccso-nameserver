  register char *cp;
  static char value[8192];
  register char *vp;

  vp = value;
  for (cp=s; *cp; cp++)
    if (*cp=='\t')
    {
      *vp++ = '\\';
      *vp++ = 't';
    }
    else if (*cp=='\n')
    {
      *vp++ = '\\';
      *vp++ = 'n';
    }
    else if (*cp=='\\')
    {
      *vp++ = '\\';
      *vp++ = '\\';
    }
    else
      *vp++ = *cp;
  *vp = '\0';
  return(value);
}

/***********************************************************************
* Free a dir structure (modified for mdump)
***********************************************************************/
MdumpFreeDir(Dir *dir)
{
  char  **p;

  if (*dir)
  {
    for (p = *dir; *p; p++)
      free(*p);
    /*free(*dir);
    *dir = 0;*/
  }
}

