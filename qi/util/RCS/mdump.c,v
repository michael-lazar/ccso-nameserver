 * 
 ************************************************************************/
dump_randy(theDir)
char **theDir;
{
  printf("%s\t",escapeChars(FindValue(theDir,F_ALIAS)));
  printf("%s\t",escapeChars(FindValue(theDir,F_NAME)));
  printf("%s\t",escapeChars(FindValue(theDir,32)));
  printf("%s\n",escapeChars(FindValue(theDir,F_EMAIL)));
}

