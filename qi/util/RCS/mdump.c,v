  					FindValue(theDir,F_ALIAS));
}

/************************************************************************
 * 
 ************************************************************************/
dump_size(theDir)
char **theDir;
{
  int size=0;
  char **sp = theDir;
  
  for (sp = theDir; *sp; sp++)
  {
    size += strlen(*sp);
    size++;
  }
  printf("%d\n",size);
}

/************************************************************************
 * 
 ************************************************************************/
dump_all(theDir)
char **theDir;
{
  char *colon;
  printf("%d:%s",F_UNIVID,escapeChars(FindValue(theDir,F_UNIVID)));
  for (; *theDir; theDir++)
    if (atoi(*theDir)!=F_UNIVID && (colon=index(*theDir,':')) && colon[1])
    {
      putchar('\t');
      fputs(escapeChars(*theDir),stdout);
    }
  putchar('\n');
}

