/************************************************************************
 * 
 ************************************************************************/
dump_ruchi(theDir)
char **theDir;
{
#define PUT_OUT(num) printf("$#$%s",escapeChars(FindValue(theDir,num)))
  PUT_OUT(F_UNIVID);
  PUT_OUT(F_NAME);
  PUT_OUT(F_ALIAS);
  PUT_OUT(F_EMAIL);
  PUT_OUT(F_PHONE);
  PUT_OUT(F_ADDRESS);
  PUT_OUT(F_DEPARTMENT);
  PUT_OUT(F_TITLE);
  PUT_OUT(19);			/* office location */
  PUT_OUT(20);			/* home address */
  PUT_OUT(23);			/* nickname */
  PUT_OUT(22);			/* office address */
  PUT_OUT(32);			/* office phone */
  PUT_OUT(33);			/* home phone */
  putchar('\n');
}

